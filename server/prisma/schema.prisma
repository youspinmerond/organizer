generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Action {
  id Int @unique @id @default(autoincrement())
  name String
  description String
  day String
  date DateTime
  status Boolean
}
model Plan {
  id Int @unique @id @default(autoincrement())
  name String
  description String
  status Boolean
}